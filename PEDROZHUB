import tkinter as tk
from tkinter import ttk

root = tk.Tk()
root.title("PEDROZHUB")
root.geometry("600x400")

title = ttk.Label(root, text="PEDROZHUB - Inicializador", font=("Segoe UI", 16))
title.pack(pady=20)

root.mainloop()main.py
